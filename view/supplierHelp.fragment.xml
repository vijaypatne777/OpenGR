		descriptionKey="SupplierName">
		<filterBar>
			<f:FilterBar search="onSuppFilter">
			
				<f:filterGroupItems>
					<f:FilterGroupItem groupName="comp" groupTitle="Company Code" name="B" visibleInFilterBar="true" label="Company Code">
						<f:control>
						<m:MultiInput id="compinput" showValueHelp="true" valueHelpRequest="searchCompanyCodes" >
							
						</m:MultiInput>
						</f:control>
					</f:FilterGroupItem>
				</f:filterGroupItems>
			</f:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>